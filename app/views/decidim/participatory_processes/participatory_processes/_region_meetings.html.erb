<div class="meeting-map-header">
  <span class="meeting-map-title"><%= t(".meeting_map_title")%></span>
  <span id="meetings-map-subtitle" class="meetings-map-subtitle"><%= t(".meeting_map_subtitle")%></span>
</div>
<div id="region-meetings-map-container" class="region-meetings" data-process=<%= current_participatory_space.slug%> data-meetings-path="<%= main_app.get_all_meetings_of_a_participatory_process_path %>">
  <div id="regions" class="regions-map">
    <div id="br_mine"></div>
  </div>
  <div class="meetings-list-section meetings-not-visible" id="meetings-list-section">
    <span id="meetings-list-title" class="meetings-map-subtitle"></span>
    <div class="br-input input-highlight">
      <input type="search" class="input-group-field" placeholder="Qual reunião você procura?" aria-label="Buscar" id="searchMeetings" name="filter[search_text_cont]">

      <button class="br-button" type="submit" aria-label="Buscar">
        <i class="fas fa-search" aria-hidden="true"></i>
      </button>
    </div>
    <div id="meetings-list" class="meetings-list"></div>
  </div>
</div>